
class employee
{
	private int empid;
	private String name;
	private double salary;
	private static int cnt;
	
static
{
	cnt=0;
}

public employee()
{
	cnt++;
}

public employee(int empid,String name,double salary)
{
	this.empid=empid;
	this.name=name;
	this.salary=salary;
	cnt++;
}

public void display()
{
	System.out.println(empid+"\t"+name+"\t"+salary);
}
public static void showCnt()
{
	System.out.println("no. of objects-" +cnt);
}
}
